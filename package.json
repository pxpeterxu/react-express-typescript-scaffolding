{
  "name": "react-express-typescript-scaffolding",
  "version": "1.0.0",
  "description": "Scaffolding for use in projects, with Prettier, Typescript, ESLint, Jest tests, and Storybook already set up",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production DATABASE=production gulp build-all",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "run-dev": "cross-env DATABASE=development NODE_ENV=development",
    "run-prod": "cross-env DATABASE=production NODE_ENV=production",
    "init-db-dev": "cross-env DATABASE=development NODE_ENV=development ts-node --files server/scripts/db.ts",
    "init-db-prod": "cross-env DATABASE=production NODE_ENV=production ts-node --files server/scripts/db.ts",
    "reinit-db-dev": "cross-env DATABASE=development NODE_ENV=development ts-node --files server/scripts/setupTestDB.ts",
    "reinit-db-prod": "cross-env DATABASE=production NODE_ENV=production ts-node --files server/scripts/setupTestDB.ts",
    "test": "cross-env NODE_ENV=test DATABASE=test ts-node --files server/scripts/setupTestDB.ts && cross-env DATABASE=test jest",
    "jest": "cross-env NODE_ENV=test DATABASE=test jest",
    "jest-dev": "cross-env NODE_ENV=test DATABASE=development jest",
    "prettier": "prettier {server,common,tests,client}/**/*.{js,jsx,ts,tsx}",
    "profile": "webpack --profile --json > stats.json",
    "adjust-linux-watch-limit": "echo fs.inotify.max_user_watches=524288 | tee -a /etc/sysctl.conf && sysctl -p",
    "storybook": "start-storybook -p 9001 -s client -c .storybook",
    "b-node": "babel-node --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
